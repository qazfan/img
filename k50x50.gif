<svg version="1.1"
     width="1000" height="1000"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink">
  
  <defs>
    <g id="subject">
      <image xlink:href="https://items.jellyneo.net/assets/imgs/items/41643.gif?506" height="150" x="-75" y="-75" />
<!--       <circle fill="rgb(219, 39, 99)" r="300"/>
      <text
        fill="rgb(176, 219, 67)"
        font-size="202"
        style="stroke:#000;stroke-width: 4px;"
        alignment-baseline="middle"
        text-anchor="middle"
      >♛♞</text>
      <text
        fill="rgb(18, 234, 234)"
        font-size="252"
        style="stroke:#000;stroke-width: 4px;"
        alignment-baseline="middle"
        text-anchor="middle"
            transform="rotate(90)"
      >ฒผู้</text> -->
      <animateTransform
        attributeName="transform"
        attributeType="XML"
        type="rotate"
        from="0"
        to="360"
        dur="4s"
        repeatCount="indefinite"
      />
    </g> 
    <clipPath id="triangle">
      <polygon points="0 0, 0 50, 29 50" fill="#000"/>
    </clipPath>
    <g id="single">  
      <use href="#subject" clip-path="url(#triangle)" />
    </g>
    <g id="double">
      <use href="#single"/>
      <use href="#single" transform-origin="0 0" transform="scale(-1, 1)"/>
    </g>
    <g id="triangles" transform="translate(25 25)">
      <use href="#double"/>
      <use href="#double" transform="rotate(60)"/>
      <use href="#double" transform="rotate(120)"/>
      <use href="#double" transform="rotate(180)"/>
      <use href="#double" transform="rotate(240)"/>
      <use href="#double" transform="rotate(300)"/>
    </g>
    <clipPath id="square50x50">
      <rect width="50" height="50" fill="black"/>
    </clipPath>
  </defs>
  <!-- <rect fill="url(#tiles)" stroke="black" width="1000" height="1000"/> -->
  <g clip-path="url(#square50x50)">
    <use href="#triangles"/>
  </g>
  

</svg>